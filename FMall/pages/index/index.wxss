  .f-grid:active {
    background-color: #ECECEC;
}  
.f-grid-bg {
    position: relative;
    float: left;
    padding: 0px 0px;
    width: 100%;
    box-sizing: border-box;
}
.f-grid-icon {
    width: 45px;
    height: 45px;
    margin: 0 auto;
}
.f-grid-icon image {
    display: block;
    width: 100%;
    height: 100%;
    border-radius: 30px; 
}
.f-grid-icon + .f-grid-label {
    margin-top: 5px;
}
.f-grid-label {
    display: block;
    text-align: center;
    color: #999;
    font-size: 11px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
/*tab*/
.f-tab{
    background: #FFFFFF;
    margin-top:10px; 
}
.content {
  padding: 20px;
  background-color: #fff;
}