page{
    height: 100%;
    width: 100%;
    background: #f4f4f4;
}
.container{
    background: #f4f4f4;
    height: auto;
    overflow: hidden;
    width: 100%;
}
.userinfo {
	display: flex;
	/* flex-direction: column; */
	/* padding: 20rpx 0; */
	/* align-items: center; */
	background: #ffffff;
  width: 750rpx; 

}
.userbutton{
  background: #ffffff;
}

.userinfo-avatar {
	width: 160rpx;
	height: 160rpx;
	margin: 20rpx 0;
	border-radius: 50%;
}

.userinfo-nickname {
  display: block;
  color:#333; 
  font-weight:bold;
  font-size:18px;
  margin-top: 38rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align:left;
  margin-left:-10rpx;


}

.userinfo-mobile { 
  height:50rpx;
  margin-top: 20rpx;

}
.userinfo-mobile .a{
  display: flex; 
  flex-direction: row; 
  justify-content: center;
}
.userinfo-mobile .txt{  
  height: 50rpx; 
  line-height: 40rpx;
  font-size: 30rpx;
  color:#b4282d;
  border: 1px solid #b4282d;
  border-radius: 25rpx;
  padding: 5rpx 20rpx;
  margin-top:5rpx;
}
/* 我的订单 */
.my_order{
  width: 100%;
  background: #ffffff;
  height: 50px;
  line-height: 50px;
  border-bottom:1px solid #f4f4f4;
  /* display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center; */
}
.cont{
  margin: 0 20rpx;
}
.ordergo{
  display: inline-block;
  height: 26rpx;
  width: 16rpx;
  padding-top:3rpx; 
}
.l{
  float: left; 
}
.r{
  float: right; 
}
.order_detai{
  clear: both;
  width: 100%;
}
.order_detai .li{
  float: left;
  width: 25%;
  height: 160rpx;
  text-align: center;
}

.order_detai .li .a{
  clear: both;
  display:flex;
  width:100%;
  height:100%;
  flex-direction:column;
  align-items:center;
  justify-content:center;
  background: #ffffff;

}
.order_detai .icon{
  margin: 0 auto;
  display: block;
  height: 52.803rpx;
  width: 52.803rpx;
  margin-bottom: 5rpx;
}
